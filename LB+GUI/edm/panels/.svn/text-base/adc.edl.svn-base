showXAxis
xAxisSrc "fromUser"
xMin 0
xMax 2048
xLabelIntervals 4
xMajorsPerLabel 1
xMinorsPerMajor 8
xShowLabelGrid
xShowMajorGrid
xShowMinorGrid
xUserSpecScaleDiv 1
xAxisSmoothing 1
# Y axis properties
showYAxis
yAxisSrc "AutoScale"
yMax 1
yLabelIntervals 4
yMajorsPerLabel 1
yMinorsPerMajor 4
yShowLabelGrid
yShowMajorGrid
yShowMinorGrid
yUserSpecScaleDiv 1
# Y2 axis properties
y2AxisSrc "AutoScale"
y2Max 1
# Trace Properties
numTraces 4
yPv {
  0 "EPICS\\$(HOSTNAME):$(BPM):$(DD).VALA"
  1 "EPICS\\$(HOSTNAME):$(BPM):$(DD).VALB"
  2 "EPICS\\$(HOSTNAME):$(BPM):$(DD).VALC"
  3 "EPICS\\$(HOSTNAME):$(BPM):$(DD).VALD"
}
ySigned {
  0 1
  1 1
  2 1
  3 1
}
plotColor {
  0 index 16
  1 index 83
  2 index 46
  3 index 43
}
endObjectProperties

